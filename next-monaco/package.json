{
  "name": "next-monaco",
  "version": "0.0.0",
  "description": "Monaco Editor x Next.js",
  "files": [
    "dist/*"
  ],
  "type": "module",
  "main": "./dist/server/index.js",
  "exports": {
    ".": "./dist/server/index.js",
    "./plugin": "./dist/plugin/index.js"
  },
  "types": "./dist/server/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/server/index.d.ts"
      ],
      "plugin": [
        "./dist/plugin/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup plugin/index.ts setup/index.ts client/index.tsx server/index.ts --dts --format esm",
    "dev": "pnpm build --watch",
    "test": "jest"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "next": "13.4.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "webpack": "5.88.2"
  },
  "dependencies": {
    "@code-hike/lighter": "^0.8.2",
    "@next/mdx": "^13.5.2",
    "@types/mdx": "^2.0.7",
    "@typescript/ata": "^0.9.4",
    "copy-webpack-plugin": "^11.0.0",
    "monaco-editor": "^0.37.1",
    "node-fetch": "^3.3.2",
    "rollup": "^3.29.3",
    "rollup-plugin-dts": "^6.0.2",
    "server-only": "^0.0.1",
    "typescript": "^5.2.2",
    "vscode-oniguruma": "^2.0.1",
    "vscode-textmate": "^9.0.0",
    "vscode": "npm:@codingame/monaco-vscode-api@1.78.0",
    "yauzl": "^2.10.0"
  }
}
